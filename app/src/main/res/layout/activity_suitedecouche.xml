<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/salleinter"
    tools:context=".suitedecouche">

    <Button
        android:id="@+id/buttons2"
        android:layout_width="333dp"
        android:layout_height="0dp"
        android:layout_marginTop="244dp"
        android:background="@drawable/buttonpink"
        android:fontFamily="@font/nunito_extrabold"
        android:text="Surveillance de la mère"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttons4"
        android:layout_width="333dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/buttonpink"
        android:fontFamily="@font/nunito_extrabold"
        android:text="Soins à la mère"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttons2" />

    <Button
        android:id="@+id/buttons7"
        android:layout_width="333dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/buttonpink"
        android:fontFamily="@font/nunito_extrabold"
        android:text="Allaitement"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttons10" />

    <Button
        android:id="@+id/buttons9"
        android:layout_width="333dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/buttonpink"
        android:fontFamily="@font/nunito_extrabold"
        android:text="Conseil et orientation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttons7" />

    <Button
        android:id="@+id/buttons10"
        android:layout_width="333dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/buttonpink"
        android:fontFamily="@font/nunito_extrabold"
        android:text="Surveillance du N-Né"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttons4" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="260dp"
        android:layout_height="61dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="96dp"
        android:fontFamily="@font/inter"
        android:text="Choisissez une catégorie pour consulter les soins aprés l'accouchement"
        android:textAlignment="center"
        android:textColor="#FEFEFE"
        app:layout_constraintBottom_toTopOf="@+id/buttons2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="sans-serif-black"
        android:text="service Suite de couches"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>